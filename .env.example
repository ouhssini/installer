APP_NAME=Laravel
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost
APP_TIMEZONE=UTC

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

# Will be generated by the installer
APP_KEY=

LOG_CHANNEL=stack
LOG_LEVEL=debug

# ----------------------------
# Installer-safe defaults
# ----------------------------

SESSION_DRIVER=file
SESSION_LIFETIME=120

CACHE_STORE=file

QUEUE_CONNECTION=sync

FILESYSTEM_DISK=local

BROADCAST_CONNECTION=log

# ----------------------------
# Database (configured later)
# ----------------------------

DB_CONNECTION=
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=

# ----------------------------
# Mail (safe default)
# ----------------------------

MAIL_MAILER=log
MAIL_FROM_ADDRESS=hello@example.com
MAIL_FROM_NAME="${APP_NAME}"

# ----------------------------
# Optional / future
# ----------------------------

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

VITE_APP_NAME="${APP_NAME}"
